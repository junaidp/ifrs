<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder
 xmlns:ui="urn:ui:com.google.gwt.uibinder"
 	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.ifrs.client.view">
	<g:HTMLPanel>
	<m:MaterialRow>
	<m:MaterialColumn grid="s12">
	<m:MaterialTab ui:field="tab" indicatorColor="YELLOW" shadow="1" grid = "s12">
	<m:MaterialTabItem grid="s3" waves="YELLOW" >
	<m:MaterialLink ui:field="linkSector" text="Add Sector" href="#tab1" textColor="WHITE" waves="LIGHT">
	</m:MaterialLink>
	</m:MaterialTabItem>
	<m:MaterialTabItem grid="s3" waves="YELLOW" >
	<m:MaterialLink ui:field="linkCompany" text="Add Company" href="#tab2" textColor="WHITE" waves="LIGHT">
	</m:MaterialLink>
	</m:MaterialTabItem>
	<m:MaterialTabItem grid="s3" waves="YELLOW">
	<m:MaterialLink ui:field="linkDomain" text="Add Domain"  href="#tab3" textColor="WHITE" waves="LIGHT"></m:MaterialLink>
	</m:MaterialTabItem>
	<m:MaterialTabItem grid="s3" waves="YELLOW" >
	<m:MaterialLink ui:field="linkSubDomain" text="Add SubDomain" href="#tab4" textColor="WHITE" waves="LIGHT">
	</m:MaterialLink>
	</m:MaterialTabItem>
	</m:MaterialTab>
	</m:MaterialColumn>
	
	<m:MaterialColumn m:id="tab1" grid="s12">
	
	<m:MaterialButton text="add sector" ui:field="btnAddSector" ></m:MaterialButton>
	<m:MaterialRow>
	<m:MaterialColumn  ui:field="containerSector">
	</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow>
	<m:MaterialButton text="Save Sector" ui:field="btnSaveSector"></m:MaterialButton>
	</m:MaterialRow>
	</m:MaterialColumn>
	<m:MaterialColumn m:id="tab2" grid="s12">
		<m:MaterialButton text="Add Company" ui:field="btnAddCompany" ></m:MaterialButton>
		<m:MaterialRow>
		<m:MaterialColumn  ui:field="containerCompany">
		</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow>
		
		<m:MaterialButton text="Save Company" ui:field="btnSaveCompany"></m:MaterialButton>
	</m:MaterialRow>
	</m:MaterialColumn>
	
	<m:MaterialColumn m:id="tab3" grid="s12" ui:field="containerDomain">
	</m:MaterialColumn>
	<m:MaterialColumn m:id="tab4" grid="s12" ui:field="containerSubDomain">
	</m:MaterialColumn>

	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 